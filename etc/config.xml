<?xml version="1.0"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Store:etc/config.xsd">
    <default>
        <payment>
            <amwal_payments>
                <ref_id_secret backend_model="Magento\Config\Model\Config\Backend\Encrypted" />
                <title>Amwal</title>
                <order_status>processing</order_status>
                <merchant_mode>live</merchant_mode>
                <model>AmwalPaymentMethodFacade</model>
                <group>amwal_payments_group</group>
                <prod_api_base_url><![CDATA[https://backend.sa.amwal.tech]]></prod_api_base_url>
                <test_api_base_url><![CDATA[https://qa-backend.sa.amwal.tech]]></test_api_base_url>
                <can_use_checkout>1</can_use_checkout>
            </amwal_payments>
        </payment>
    </default>
</config>
