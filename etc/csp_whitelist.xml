<?xml version="1.0"?>
<csp_whitelist xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Csp:etc/csp_whitelist.xsd">
    <policies>
        <policy id="script-src">
            <values>
                <value id="amwal-lib-base" type="host">https://cdn.jsdelivr.net</value>
                <value id="amwal-other-base" type="host">https://cdnjs.cloudflare.com</value>
                <value id="amwal" type="host">*.amwal.tech</value>
                <value id="unpkg" type="host">https://unpkg.com</value>
                <value id="oppwa" type="host">https://eu-test.oppwa.com</value>
                <value id="ppipe" type="host">https://test.ppipe.net</value>
                <value id="shpf-base" type="host">https://secure.iframe.com</value>
                <value id="iframe" type="host">https://iframe.com</value>
                <value id="sentry" type="host">*.sentry.io</value>
            </values>
        </policy>
        <policy id="frame-src">
            <values>
                <value id="amwal-lib-base" type="host">https://cdn.jsdelivr.net</value>
                <value id="amwal-other-base" type="host">https://cdnjs.cloudflare.com</value>
                <value id="amwal" type="host">*.amwal.tech</value>
                <value id="unpkg" type="host">https://unpkg.com</value>
                <value id="oppwa" type="host">https://eu-test.oppwa.com</value>
                <value id="ppipe" type="host">https://test.ppipe.net</value>
                <value id="ppipe" type="host">https://eu-test.ppipe.net</value>
                <value id="shpf-base" type="host">https://secure.iframe.com</value>
                <value id="iframe" type="host">https://iframe.com</value>
            </values>
        </policy>
        <policy id="connect-src">
            <values>
                <value id="amwal" type="host">*.amwal.tech</value>
                <value id="unpkg" type="host">https://unpkg.com</value>
                <value id="sentry-ingest" type="host">*.sentry.io</value>
            </values>
        </policy>
        <policy id="style-src">
            <values>
                <value id="googleapis-fonts" type="host">*.googleapis.com</value>
            </values>
        </policy>
        <policy id="font-src">
            <values>
                <value id="googleapis-fonts" type="host">*.gstatic.com</value>
            </values>
        </policy>
    </policies>
</csp_whitelist>
